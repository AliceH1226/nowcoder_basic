//https://ac.nowcoder.com/acm/contest/549/D



#include<iostream>
#include<algorithm>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)cin>>a[i];
    sort(a,a+n);
    long long ans=0;
    int cnt=0;
    for(int i=n-1;i>=0;i--)
    {
        if((ans|a[i])>ans&&cnt<n-1)
        {
            cnt++;
            ans|=a[i];
        }

    }
    cout<<ans<<endl;
}
